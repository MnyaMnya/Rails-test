<h4>Log In</h4>
<%= form_with model: @user, url: session_path do |form| %>
    

    <div>
        <%= form.label :email %>
        <%= form.email_field :email %>
    </div>

    <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
    </div>


    <div>
        <%= form.submit "Log In" %>
    </div>
<% end %>

<%= link_to "Forget your password?", new_password_reset_path%>